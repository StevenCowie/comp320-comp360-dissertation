	pages = {66--69},
	file = {ACM Full Text PDF:C\:\\Users\\steve\\Zotero\\storage\\6ZHP9JY9\\Gerling et al. - 2010 - Exergame Design for Elderly Users The Case Study .pdf:application/pdf}
